<Patch>
	<Operation Class="PatchOperationRemove">
		<xpath>
			Defs/ThingDef[defName="Axolotl_MoeLotlCrossBow_Basic"
			or defName="Axolotl_MoeLotlCrossBow_Intermediate"
			or defName="Axolotl_MoeLotlCrossBow_Advanced"
			or defName="Axolotl_FallingSunBow"
			or defName="Axolotl_BattlebreakerCrossbow"
			or defName="Axolotl_BlackGoldFirearm"
			or defName="Axolotl_PlatinumFirearm"]/verbs
		</xpath>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>
			Defs/ThingDef[defName="Axolotl_MoeLotlCrossBow_Basic"
			or defName="Axolotl_MoeLotlCrossBow_Intermediate"
			or defName="Axolotl_MoeLotlCrossBow_Advanced"
			or defName="Axolotl_FallingSunBow"
			or defName="Axolotl_BattlebreakerCrossbow"
			or defName="Axolotl_BlackGoldFirearm"
			or defName="Axolotl_PlatinumFirearm"
			]/comps
		</xpath>
		<value>
			<li>
				<compClass>AxolotlCE.CompQiWeaponStatCE</compClass>
			</li>
		</value>
	</Operation>
<!-- 一般连弩 -->
